{
  "name": "logseq-uri-link",
  "displayName": "Logseq URI Link",
  "description": "This extension allows you to click on logseq hyperlinks in vscode",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.82.0",
    "azureDataStudio": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onStartupFinished"],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "extension.openLink",
      "title": "Open Link"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
